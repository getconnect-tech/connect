{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensions": [".js", ".ts", ".jsx", ".tsx"]
  },
  "editor.formatOnSave": true,
  "cSpell.words": [
    "datetime",
    "ONESIGNAL",
    "TEAMCAMP"
  ]
}
